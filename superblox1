-- Scripts:

local function SJZTRYS_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local onbutton = script.Parent.Frame.on
	local offbutton = script.Parent.Frame.off
	
	onbutton.MouseButton1Click:Connect(function()
		script.Parent.testtitle.Text = "SUS"
	end)
	offbutton.MouseButton1Click:Connect(function()
		script.Parent.testtitle.Text = "SUSSY"
	end)
end
coroutine.wrap(SJZTRYS_fake_script)()
local function SHSF_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	-- HOW TO DO --
	-- 1. Insert a ScreenGui then drag it into the StarterGui
	-- 2. Insert a Frame
	-- 3. Insert TextButton
	-- 4. Insert a Local Script inside the TextButton
	
	-- LocalScript:
	
	local Open = false
	local Frame = script.Parent.Frame
	local Button = script.Parent.TextButton
	
	function Clicked()
		if Open == false then
			Open = true
			Frame.Visible = true
			Button.Text = "Close" -- Closes the Frame
		elseif Open == true then
			Open = false
			Frame.Visible = false
			Button.Text = "Open" -- Opens the Frame
		end
	end
	
	
	Button.MouseButton1Click:connect(Clicked)
end
coroutine.wrap(SHSF_fake_script)()
local function OUANHB_fake_script() -- ScreenGui.LocalScript 
	local script = Instance.new('LocalScript', ScreenGui)

	local onbutton = script.Parent.Frame.on
	local offbutton = script.Parent.Frame.off
	
	
	while true do 
		if script.Parent.testtitle.Text == "SUS" then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 21
			game.Players.LocalPlayer.Character.Humanoid:MoveTo(workspace.Ball.Position)
			if script.Parent.testtitle.Text == "SUSSY" then
				break
	
			end
		end
		if script.Parent.testtitle.Text == "SUSSY" then
			game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16
			
		end
		wait(0.1)
	end
end
coroutine.wrap(OUANHB_fake_script)()
